    "components": {
        "schemas": {
            "ChapterInfo": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "Id"
                    },
                    "title": {
                        "type": "string",
                        "title": "Title"
                    },
                    "image": {
                        "type": "string",
                        "title": "Image",
                        "default": ""
                    },
                    "milestones": {
                        "items": {
                            "$ref": "#/components/schemas/Milestone"
                        },
                        "type": "array",
                        "title": "Milestones"
                    },
                    "description": {
                        "type": "string",
                        "title": "Description",
                        "default": ""
                    },
                    "timestamp": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Timestamp"
                    }
                },
                "type": "object",
                "required": [
                    "id",
                    "title",
                    "milestones"
                ],
                "title": "ChapterInfo"
            },
            "ChapterMilestone": {
                "properties": {
                    "milestoneId": {
                        "type": "integer",
                        "title": "Milestoneid"
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "matches": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array",
                        "title": "Matches"
                    },
                    "completed": {
                        "type": "boolean",
                        "title": "Completed"
                    }
                },
                "type": "object",
                "required": [
                    "milestoneId",
                    "name",
                    "matches",
                    "completed"
                ],
                "title": "ChapterMilestone"
            },
            "Character": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "Id"
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "subtitle": {
                        "type": "string",
                        "title": "Subtitle",
                        "default": ""
                    },
                    "image": {
                        "type": "string",
                        "title": "Image",
                        "default": ""
                    },
                    "description": {
                        "type": "string",
                        "title": "Description",
                        "default": ""
                    },
                    "type": {
                        "type": "string",
                        "title": "Type",
                        "default": "npc"
                    },
                    "timestamp": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Timestamp"
                    }
                },
                "type": "object",
                "required": [
                    "id",
                    "name"
                ],
                "title": "Character"
            },
            "CreateDialogRequest": {
                "properties": {
                    "activeChapterId": {
                        "type": "integer",
                        "title": "Activechapterid"
                    },
                    "playerQuestion": {
                        "type": "string",
                        "title": "Playerquestion"
                    },
                    "completedChapterIds": {
                        "items": {
                            "type": "integer"
                        },
                        "type": "array",
                        "title": "Completedchapterids"
                    },
                    "milestones": {
                        "items": {
                            "$ref": "#/components/schemas/ChapterMilestone"
                        },
                        "type": "array",
                        "title": "Milestones"
                    }
                },
                "type": "object",
                "required": [
                    "activeChapterId",
                    "playerQuestion",
                    "completedChapterIds",
                    "milestones"
                ],
                "title": "CreateDialogRequest"
            },
            "GameStoryModel": {
                "properties": {
                    "storyId": {
                        "type": "integer",
                        "title": "Storyid"
                    },
                    "title": {
                        "type": "string",
                        "title": "Title"
                    },
                    "lore": {
                        "type": "string",
                        "title": "Lore",
                        "default": ""
                    },
                    "characters": {
                        "items": {
                            "$ref": "#/components/schemas/Character"
                        },
                        "type": "array",
                        "title": "Characters"
                    },
                    "chapters": {
                        "items": {
                            "$ref": "#/components/schemas/ChapterInfo"
                        },
                        "type": "array",
                        "title": "Chapters"
                    },
                    "timestamp": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Timestamp"
                    }
                },
                "type": "object",
                "required": [
                    "storyId",
                    "title",
                    "characters",
                    "chapters"
                ],
                "title": "GameStoryModel"
            },
            "GenerateDialogTree": {
                "properties": {
                    "projectId": {
                        "type": "integer",
                        "title": "Projectid"
                    },
                    "dialogTreeId": {
                        "type": "integer",
                        "title": "Dialogtreeid"
                    },
                    "currentChapterId": {
                        "type": "integer",
                        "title": "Currentchapterid"
                    },
                    "currentDialogTreeNodeId": {
                        "type": "integer",
                        "title": "Currentdialogtreenodeid"
                    },
                    "toDialogAgentId": {
                        "type": "integer",
                        "title": "Todialogagentid"
                    }
                },
                "type": "object",
                "required": [
                    "projectId",
                    "dialogTreeId",
                    "currentChapterId",
                    "currentDialogTreeNodeId",
                    "toDialogAgentId"
                ],
                "title": "GenerateDialogTree"
            },
            "GenerateInstantRequest": {
                "properties": {
                    "projectId": {
                        "type": "integer",
                        "title": "Projectid"
                    },
                    "fromId": {
                        "type": "integer",
                        "title": "Fromid"
                    },
                    "toId": {
                        "type": "integer",
                        "title": "Toid"
                    },
                    "fromInput": {
                        "type": "string",
                        "title": "Frominput"
                    },
                    "current_chapter_id": {
                        "type": "integer",
                        "title": "Current Chapter Id"
                    },
                    "completed_chapter_ids": {
                        "items": {
                            "type": "integer"
                        },
                        "type": "array",
                        "title": "Completed Chapter Ids"
                    }
                },
                "type": "object",
                "required": [
                    "projectId",
                    "fromId",
                    "toId",
                    "fromInput",
                    "current_chapter_id",
                    "completed_chapter_ids"
                ],
                "title": "GenerateInstantRequest"
            },
            "GoogleLoginRequest": {
                "properties": {
                    "id_token": {
                        "type": "string",
                        "title": "Id Token"
                    }
                },
                "type": "object",
                "required": [
                    "id_token"
                ],
                "title": "GoogleLoginRequest"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "LoginResponse": {
                "properties": {
                    "kind": {
                        "type": "string",
                        "title": "Kind"
                    },
                    "localId": {
                        "type": "string",
                        "title": "Localid"
                    },
                    "email": {
                        "type": "string",
                        "title": "Email"
                    },
                    "displayName": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Displayname"
                    },
                    "idToken": {
                        "type": "string",
                        "title": "Idtoken"
                    },
                    "registered": {
                        "type": "boolean",
                        "title": "Registered"
                    },
